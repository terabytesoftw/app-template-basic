{
    "name": "terabytesoftw/app-template-basic",
    "type": "project",
    "description": "Yii 2.0 Web Application Basic",
    "keywords": [
        "application",
        "basic",
        "web",
        "yii"
    ],
    "homepage": "https://github.com/terabytesoftw/app-template-basic",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "wilmer.arambula@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "terabytesoftw/app-basic": "^1.0@dev"
    },
    "require-dev": {
        "codeception/codeception": "^3.0",
        "codeception/c3": "2.*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "dms/phpunit-arraysubset-asserts": "^0.1.0",
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "monolog/monolog": "*",
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "autoload": {
        "psr-4": {"terabytesoft\\app\\template\\basic\\": ""}
    },
    "autoload-dev": {
        "psr-4": {"terabytesoft\\app\\template\\basic\\tests\\": "tests/"}
	},
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": [
                "config/appbasic.php",
                "config/params.php"
            ],
            "console": "config/console.php",
            "web": "config/web.php"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
